pipeline {
    agent any

    stages {
        stage('Compile') {
            steps {
                echo 'Compile the code'
            }
        }
            stage('Test') {
            steps {
                echo 'Test the code'
            }
            }
            stage('Package') {
            steps {
                echo 'Packaging the code'
            }
            }
        }

 

}
